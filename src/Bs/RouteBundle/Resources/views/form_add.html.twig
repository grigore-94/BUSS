{% form_theme form 'bootstrap_3_horizontal_layout.html.twig' %}
{{ form_start(form) }}
{{ form_row(form.from) }}
{{ form_row(form.to) }}
{{ form_row(form.activeDays) }}
{{ form_row(form.hourDeparture) }}
{{ form_row(form.road) }}

<input type="hidden" id="placeIdArray" name="placeIdArray" value="">
<label>Select The road:</label>
<div id="map" style="height: 600px; width: 1000px;left: 60px;"></div>
<div id="bar">
    <p class="auto"><input type="text" id="autoc"/></p>
    <p><a id="clear" href="#">Click here</a> to clear map.</p>
</div>
    {% include('@BsRoute/map.html.twig') %}


<button type="submit" class="btn btn-default">Save</button>
{{ form_end(form) }}